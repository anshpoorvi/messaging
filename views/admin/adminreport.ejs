<div class="report-container">
        <div class="report-filter">
      <div class="row">
            <div class="col-sm-12 col-xs-12">
             
             <form id="findUserForm" action="/admin/findUser" method="POST"> 
                <div class="col-sm-3 col-xs-12">
                    <label class="control-label">User name</label>
                    <input type="text" id="seeker" name="userName" class="form-control internal-form" autocomplete="off" />
                     
                    <div class="field-validation-error"></div>
                </div>
                <div class="col-sm-3 col-xs-12">
                    <label class="control-label">Status</label>
                    <select name="status" class="form-control">
                        <option value="Block">Block</option>
                        <option value="Unblock">Active</option>
                        <option value="All">All</option>
                    </select>
                    <div class="field-validation-error"></div>
                </div>
                <div class="col-sm-6 col-xs-12">
                  <label class="control-label">&nbsp;</label>
                  <button type="submit" title="Search"  class="button button--moema button--moema-white button--moema-white-shadow button--inverted button--text-thick button--size-s button--moema-small">Search</button>
                  <a href="/admin/createExcel" title="Export to excel" class="button button--moema button--moema-white button--moema-white-shadow button--inverted button--text-thick button--size-s button--moema-small">Export to excel</a>
                </div>
              </form>    
            </div>
        </div>
      </div>
      
      <div class="table-responsive">
      <div class="col-sm-12 col-xs-12">
       
      <table class="table dataTable table-bordered" id="reportTable">
            <thead>
              <tr>
                <th>User name</th>
                <th>Email</th>
                <th>Total Message</th>
                <th>Send Message</th>
                <th>Status</th>
              </tr>
            </thead>
            <tbody>
              <% data.securedUserMsg.forEach(function(suser){%>
              <tr>
                <td><%= suser.full_name %></td>
                <td><%= suser.email %></td>
                <td><%= suser.total_messages %></td>
                <td><%= suser.send_messages %></td>
                <% if(suser.active==0) {%>
                <td>Block</td>
                <%}else{%>
                  <td>Active</td>
                  <%}%>
              </tr>
              <%})%>
            </tbody>
          </table>
      </div>
      </div>
      </div>
      