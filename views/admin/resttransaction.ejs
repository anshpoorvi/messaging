
<% var month = [];
month[0] = "January";
month[1] = "February";
month[2] = "March";
month[3] = "April";
month[4] = "May";
month[5] = "June";
month[6] = "July";
month[7] = "August";
month[8] = "September";
month[9] = "October";
month[10] = "November";
month[11] = "December"; 
%>
<br>
<br> 
<div id="messageContainer">
<div class="messageGrid-container">
<div class="table-responsive">
<div class="col-sm-12 col-xs-12">
<div class="field-validation-error">
<%=data.error%>
</div>
<%if(!data.error){%>
<table class="table dataTable table-bordered" id="msgTable" >
<thead>
<tr>
<th>Transation ID</th>
<th>Sender</th>
<th>Receiver</th>
<th>Message</th>
</tr>
</thead>
<tbody>
<% data.history.forEach(function(data){
var sender = data.sender.split("#"); 
var receiver = data.receiver.split("#");
var message = data.message.split("#");
%>
<tr>
<td><%=data.transactionId%></td>
<td><%= sender[1] %></td>
<td><%= receiver[1]%></td>
<td><%-message[1]%></td>
</tr>
<%});%>
</tbody>
</table>
<%}%>
</div>
</div>
</div>
</div>

